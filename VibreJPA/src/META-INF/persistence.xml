<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="VibreJPA" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/vibration</jta-data-source>
		<class>vibration.JPA.Experimenten</class>
		<class>vibration.JPA.Foto</class>
		<class>vibration.JPA.Locaty</class>
		<class>vibration.JPA.Meting</class>
		<class>vibration.JPA.Personen</class>
		<class>vibration.JPA.Project</class>
		<properties>
			<property name="eclipselink.logging.level" value="INFO"/>
		</properties>
	</persistence-unit>
</persistence>
